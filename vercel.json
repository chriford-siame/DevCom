{
    "version": 2,
    "builds": [
      {
        "src": "public/index.php",
        "use": "@vercel/php",
        "config": {
          "installCommand": "composer install --no-dev --optimize-autoloader"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/public/index.php"
      }
    ],
    "env": {
      "APP_ENV": "production",
      "APP_DEBUG": "false",
      "APP_KEY": "@app_key",
      "DB_CONNECTION": "mysql",
      "DB_HOST": "localhost",
      "DB_PORT": "3306",
      "DB_DATABASE": "laravel",
      "DB_USERNAME": "root",
      "DB_PASSWORD": "greenlight"
    }
  }
  